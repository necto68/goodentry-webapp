  "source": "src/index.html",
  "scripts": {
    "build": "yarn build:abi && yarn build:ts",
    "build:abi": "typechain --target=ethers-v5 --out-dir=./src/features/smart-contracts/types ./src/features/smart-contracts/interfaces/**.json",
    "build:ts": "parcel build",
    "lint": "yarn lint:ts && yarn lint:eslint",
    "lint:eslint": "eslint --ext .ts,.tsx,.html ./src",
    "lint:ts": "tsc --noEmit",
    "pre-commit": "yarn lint && lint-staged",
    "start": "yarn build:abi && parcel --port 1234",
    "prepare": "husky install"
  },
